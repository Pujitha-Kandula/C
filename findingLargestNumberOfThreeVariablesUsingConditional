#include <stdio.h>

int main()
{
    int a,b,c;
    printf("Enter any three values: ");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b && a>c)
    {
        printf("The largest value is %d",a);
    }
    else if(b>c)
    {
        printf("The largest value is %d",b);
    }
    else
    {
        printf("The largest value is %d",c);
    }
    return 0;
}
